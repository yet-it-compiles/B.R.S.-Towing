{
  "extends": "stylelint-config-standard-scss",
  "plugins": ["stylelint-scss"],
  "rules": {
    "block-no-empty": null,
    "color-no-invalid-hex": true,
    "no-duplicate-selectors": true,
    "scss/dollar-variable-pattern": "^[_a-z]+[a-z0-9-]*$",
    "scss/load-partial-extension": "always"
  }
}
